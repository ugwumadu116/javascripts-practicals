function getPrimes(max) {
    let sieve = []; 
    let primes = [];
    for (let i = 2; i <= max; i++) {
        if (!sieve[i]) {
            // i has not been marked -- it is prime
            primes.push(i);
            for (let j = i; j <= max; j += i) {
                sieve[j] = true;
            }
        }
    }   
    return primes.reduce((acc, step)=>{return acc + step },0)
}

console.log(getPrimes(10))
