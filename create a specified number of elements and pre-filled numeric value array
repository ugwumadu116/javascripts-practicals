console.log(array_filled(4, 11));
[11, 11, 11, 11]*/

/*
function array_filled(itemNum, item){
	let ans = [];
	for(let i = 0; i <= itemNum; i++){
		ans.push(item)
	}
	return ans
}
console.log(array_filled(4, 11));*/
function array_filled(len, val){
    return Array(len).fill(val);
}
console.log(array_filled(4, 11)); 
